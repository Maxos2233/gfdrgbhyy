#include <iostream>
using namespace std;

int main()
{
    int sum = 0;
    int i, j, cnt;

    for (i = 2; i < 1000; ++i) {
        cnt = 0;
        for (j = 1; j <= i; ++j) {
            if (!(i % j))
                ++cnt;
        }
        if (cnt == 2) {
            sum += i;
            cout << i << " ";
        }
    }
    cout << "\nSum = " << sum << endl;
    return 0;
}
